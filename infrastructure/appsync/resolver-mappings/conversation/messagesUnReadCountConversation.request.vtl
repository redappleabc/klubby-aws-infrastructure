{
    "version" : "2017-02-28",
    "operation" : "Query",
    "query" : {
        "expression": "conversationId = :id and id > :read ",
        "expressionValues" : {
            ":id" : {
                "S" : "${context.source.id}"
            },
             ":read" : {
                "S" : "${ctx.prev.result}"
            }
        }
    },
    "scanIndexForward": false
}