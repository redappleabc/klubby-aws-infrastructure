{
    "version" : "2017-02-28",
    "operation" : "Query",
    "query" : {
        "expression": "username = :username",
        "expressionValues" : {
            ":username" : {
                "S" : "${context.identity.username}"
            }
        }
    },
    "scanIndexForward": false,
    "limit": 1000
}